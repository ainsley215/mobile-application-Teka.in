<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:id="@+id/awal_"
	android:background="@drawable/selector_background"
	tools:context=".Awal">

	<View
		android:id="@+id/_bg__awal_"
		android:background="@drawable/_bg__awal__shape"
		android:layout_width="@dimen/_411sdp"
		android:layout_height="@dimen/_846sdp"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintBottom_toBottomOf="parent" />

	<ImageView
		android:id="@+id/gambar"
		android:layout_width="@dimen/_0sdp"
		android:layout_height="@dimen/_0sdp"
		android:src="@drawable/gambar"
		android:contentDescription="gambar"
		android:scaleType="centerCrop"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent" />

	<View
		android:id="@+id/box"
		android:layout_width="@dimen/_0sdp"
		android:layout_height="@dimen/_846sdp"
		android:alpha="0.4"
		android:background="@drawable/box_shape"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.0"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="1.0" />

	<ImageView
		android:id="@+id/gambar_ek1"
		android:layout_width="@dimen/_400sdp"
		android:layout_height="@dimen/_400sdp"
		android:layout_marginBottom="@dimen/_24sdp"
		android:contentDescription="gambar_ek1"
		android:src="@drawable/gambar_ek1"
		app:layout_constraintBottom_toBottomOf="@+id/_bg__awal_"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.02" />

	<RelativeLayout
		android:id="@+id/group_daftar"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintBottom_toBottomOf="parent">

		<View
			android:id="@+id/daftar_"
			android:layout_width="@dimen/_218sdp"
			android:layout_height="@dimen/_38sdp"
			android:layout_marginTop="@dimen/_450sdp"
			android:alpha="0.72"
			android:layout_marginLeft="@dimen/_minus1sdp"
			android:background="@drawable/_daftar_shape" />

		<TextView
			android:id="@+id/daftar_ek1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:text="@string/daftar_ek1_string"
			android:textColor="@color/teal_blue"
			android:layout_marginTop="@dimen/_460sdp"
			android:layout_marginLeft="@dimen/_80sdp"
			android:textSize="@dimen/_15sdp"
			android:textStyle="bold" />
	</RelativeLayout>

	<RelativeLayout
		android:id="@+id/group_masuk"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintBottom_toBottomOf="parent">

		<View
			android:id="@+id/masuk_"
			android:layout_width="@dimen/_218sdp"
			android:layout_height="@dimen/_38sdp"
			android:layout_marginTop="@dimen/_300sdp"
			android:layout_marginLeft="@dimen/_minus1sdp"
			android:alpha="0.72"
			android:background="@drawable/_masuk_shape" />

		<TextView
			android:id="@+id/masuk_ek1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="@dimen/_310sdp"
			android:layout_marginLeft="@dimen/_80sdp"
			android:gravity="center"
			android:text="@string/masuk_ek1_string"
			android:textColor="@color/teal_blue"
			android:textSize="@dimen/_15sdp"
			android:textStyle="bold" />
	</RelativeLayout>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.2"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.8"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline3"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		app:layout_constraintGuide_percent="0.15"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline8"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		app:layout_constraintGuide_percent="0.35"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline9"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.15"/>

	<androidx.constraintlayout.widget.Guideline
		android:id="@+id/guideline10"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		app:layout_constraintGuide_percent="0.83" />

</androidx.constraintlayout.widget.ConstraintLayout>